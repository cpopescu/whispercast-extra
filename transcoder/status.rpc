// File status per input file.
Type TranscodingStatus {
  // [0..1]
  float progress_;
  // various messages accumulated while transcoding (errors, info, ..)
  array<string> messages_;
}

// This structure is preiodically reported by transcoder.py
// and accumulate into a TranscodingStatus
Type TranscodingProgress {
  // [0..1]
  optional float progress_;
  // info message, human readable
  optional string message_;
}
